		<column name="result" nullable="true" xsi:type="varchar" comment="result" length="255"/>
		<column name="grade" nullable="true" xsi:type="smallint" comment="grade" identity="false"/>
		<column name="questions_result" nullable="true" xsi:type="text" comment="questions_result"/>
	</table>





</schema>
